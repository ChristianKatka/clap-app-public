{
  "conpileOnSave": false,
  "compilerOptions": {
    "outDir": "./build",
    "allowJs": false,
    "target": "es5",
    "sourceMap": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "esModuleInterop": true,
    "lib": ["es2015", "es2016", "es2017"],
    "types": ["node", "aws-sdk"]
  },
  "include": ["src/**/*", "test.ts", "test"],
  "exclude": ["node_modules", "test/**/*"]
}
